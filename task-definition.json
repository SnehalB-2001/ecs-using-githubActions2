{
  "family": "task5",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mycontainer5",
      "image": "216989107004.dkr.ecr.ap-south-1.amazonaws.com/devops/ecs-project:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::216989107004:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::216989107004:role/ecs-role"
}
